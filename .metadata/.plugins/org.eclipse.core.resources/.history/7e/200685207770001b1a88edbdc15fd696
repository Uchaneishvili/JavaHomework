
public class HomeworkOne {

	public static void main(String[] args) {
		int sumOfHoundred = 0;
		int sumOfThousand = 0;
		for(int i = 0; i<100; i++) {
			sumOfHoundred = sumOfHoundred + i;
		}
		
		
		for (int i = 0; i<1000; i++) {
			sumOfThousand = sumOfThousand + i;
		}
		
		System.out.println(sumOfThousand - sumOfHoundred);
		

	}

}
