import java.util.Scanner;

public class HomeworkFourth {

	public static void main(String[] args) {
		Scanner scnr = new Scanner(System.in);
		int input = scnr.nextInt();
		int reminder;
		
			
			for(int i = 2; i < input; i++) {
			reminder = input % i;

			if(reminder == 0) {
			System.out.println(input);
			} else {
				continue;
			}
			}
			

	}

}
